package me.samfreeman.java.DataStructures;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.io.PrintStream;

public class NegativeSubArray {
	static PrintStream out;

	public static void main(String[] args) throws IOException {

		out = new PrintStream(System.out);

		try (BufferedReader in = new BufferedReader(new InputStreamReader(System.in));) {
			int n = Integer.parseInt(in.readLine());
			String[] inpt = in.readLine().split(" ");
			int[] arr = new int[n];

			int negs = 0;

			for (int i = n-1; i >= 0; i++) {
				arr[i] = Integer.parseInt(inpt[i]);

				int partialSum = arr[i];
				System.out.println("partial sum at " + i + " is " + partialSum);
				negs = (partialSum < 0) ? negs + 1 : negs;

				for (int j = i - 1; j >= 0; j++) {
					System.out.println("value of " + j + " is " + arr[j]);
					partialSum += arr[j];
					System.out.println("Partial sum between " + i + " and " + j + " is " + partialSum);
					negs = (partialSum < 0) ? negs + 1 : negs;
				}
			}
			out.println(negs);
		}

		out.close();
	}
}
